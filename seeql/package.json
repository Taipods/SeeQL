{
  "name": "seeql",
  "displayName": "SeeQL",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "seeql.createDiagram",
        "title": "SeeQL: Create a Diagram of a Create SQL Statement"
      },
      {
        "command": "seeql.createRelationalAlgebra",
        "title": "SeeQL: Create a Relational Algebra Diagram"
      },
      {
        "command": "seeql.openDb",
        "title": "SeeQL: Open Database"
      },
      {
        "command": "seeql.runQuery",
        "title": "SeeQL: Run SQL Query"
      },
      {
        "command": "seeql.generateSQLQuery",
        "title": "SeeQL: Generate SQL Query"
      },
      {
        "command": "seeql.createDB",
        "title": "SeeQL: Create Database From CSV/SQL File"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.19",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "dependencies": {
    "@mistralai/mistralai": "^1.5.1",
    "@vscode/test-electron": "^2.4.1",
    "axios": "^1.8.1",
    "csv-parser": "^3.2.0",
    "eslint": "^9.16.0",
    "mermaid": "^11.4.1",
    "node": "^18.20.7",
    "node-sql-parser": "^5.3.6",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "type-fest": "^4.33.0",
    "typescript": "^5.7.2"
  }
}
